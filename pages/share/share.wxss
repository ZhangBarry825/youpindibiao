.page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  position: absolute;
}
.page .scroll-box {
  width: 100%;
  box-sizing: border-box;
}
.page .shop-items {
  width: 100%;
  display: flex;
  flex-direction: column;
  padding-top: 10rpx;
}
.page .shop-items .item {
  background-color: #ffffff;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.page .shop-items .item .left {
  width: 146rpx;
  height: 146rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.page .shop-items .item .left image {
  width: 100%;
  height: 100%;
}
.page .shop-items .item .right {
  width: calc(100% - 146rpx);
  padding-left: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .shop-items .item .right .line1 {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.page .shop-items .item .right .line1 .title {
  font-size: 32rpx;
  color: #000000;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.page .shop-items .item .right .line1 .location {
  display: flex;
  align-items: center;
  font-size: 23rpx;
  font-weight: 400;
  color: #999999;
}
.page .shop-items .item .right .line1 .location image {
  margin: 0 10rpx;
  width: 23rpx;
  height: 30rpx;
}
.page .shop-items .item .right .line2 {
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.page .shop-items .item .right .line3 {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.page .shop-items .item .right .line3 .count {
  font-size: 23rpx;
  font-weight: 400;
  color: #999999;
}
.page .shop-items .item .right .line3 .score {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 25rpx;
  font-weight: 400;
  color: #ffa92d;
}
.page .shop-items .item .right .line3 .score image {
  margin: 0 2rpx;
  width: 27rpx;
  height: 27rpx;
}
.page .goods-box {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  background-color: #f5f5f5;
  padding-bottom: 20rpx;
}
.page .goods-box .goods-item {
  margin-top: 15rpx;
  background-color: #ffffff;
  width: 369rpx;
}
.page .goods-box .goods-item .goods-image {
  width: 369rpx;
  height: 369rpx;
}
.page .goods-box .goods-item .title {
  box-sizing: border-box;
  padding: 0 10rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .goods-box .goods-item .bottom {
  padding: 0 10rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-weight: 400;
  color: #ff2020;
}
.page .goods-box .goods-item .bottom image {
  width: 34rpx;
  height: 34rpx;
}
.page .shop-none {
  margin-top: 150rpx;
  width: 100%;
  height: 60%;
  background-color: #f4f4f4;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.page .shop-none image {
  width: 206rpx;
  height: 206rpx;
}
.page .shop-none .text {
  font-size: 30rpx;
  font-weight: 400;
  color: #030000;
  margin: 50rpx 0;
}
.page .shop-none .none-button {
  width: 212rpx;
  padding: 15rpx 0;
  border: 1px solid #f26500;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  font-weight: 400;
  color: #f26500;
}
.page .rate-class {
  margin-right: 20rpx;
}
