.page {
  height: 100%;
  width: 100%;
  position: absolute;
  background-color: #f7f7f7;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.page .tip {
  width: 100%;
  background-color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 40rpx 0;
}
.page .tip text {
  width: 333rpx;
  line-height: 1.5;
  font-size: 24rpx;
  font-weight: 400;
  color: #4d4d4d;
}
.page .buttons {
  width: 100%;
  padding: 50rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .buttons .button {
  margin: 20rpx 0;
  padding: 25rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 600rpx;
  font-size: 24rpx;
  font-weight: 400;
  border-radius: 34rpx;
}
.page .buttons .button1 {
  color: #ffffff;
  background: #f26500;
}
.page .buttons .button2 {
  background: #ebebeb;
  color: #4d4d4d;
}
