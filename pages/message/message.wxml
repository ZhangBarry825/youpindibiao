<!--pages/message/message.wxml-->
<view class="page">
    <view class="item" wx:for="{{messageList}}" wx:key="item">
        <view class="time">
            <image src="../../static/images/mes.png"></image>
            {{item.createtime}}
        </view>
        <view class="title">{{item.title}}</view>
        <view class="description">
            {{item.text}}
        </view>
    </view>

</view>
