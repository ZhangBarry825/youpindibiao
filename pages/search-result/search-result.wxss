.page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  position: absolute;
}
.page .searchInput {
  box-sizing: border-box;
  width: 100%;
}
.page .history {
  width: 100%;
  display: flex;
  padding: 30rpx;
  box-sizing: border-box;
  flex-direction: column;
}
.page .history .title {
  display: flex;
  align-items: center;
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: #666666;
  margin: 20rpx 0;
}
.page .history .title image {
  margin-right: 20rpx;
  width: 29rpx;
  height: 29rpx;
}
.page .history .items1 {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.page .history .items1 .item {
  font-size: 23rpx;
  font-weight: 400;
  color: #666666;
  padding: 5rpx 40rpx;
  box-sizing: border-box;
  border: 1px solid #bfbfbf;
  border-radius: 25rpx;
  display: flex;
  margin: 15rpx 10rpx;
  justify-content: center;
  align-items: center;
}
.page .history .items2 {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.page .history .items2 .item {
  font-size: 23rpx;
  font-weight: 400;
  color: #666666;
  padding: 5rpx 20rpx;
  box-sizing: border-box;
  border-radius: 25rpx;
  display: flex;
  margin: 15rpx 10rpx;
  justify-content: center;
  align-items: center;
}
.page .scroll-box {
  width: 100%;
  box-sizing: border-box;
}
.page .shop-box {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #f5f5f5;
  box-sizing: border-box;
  overflow: scroll;
  padding-bottom: 20rpx;
}
.page .shop-box .shop-item {
  width: 100%;
  background-color: #ffffff;
  border-bottom: 1px solid #f5f5f5;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}
.page .shop-box .shop-item .left {
  width: 146rpx;
  height: 146rpx;
  border-radius: 6rpx;
  overflow: hidden;
}
.page .shop-box .shop-item .left image {
  width: 100%;
  height: 100%;
}
.page .shop-box .shop-item .right {
  width: calc(100% - 146rpx);
  display: flex;
  flex-direction: column;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  justify-content: space-around;
}
.page .shop-box .shop-item .right .line1 {
  font-size: 33rpx;
  font-weight: 400;
  color: #000000;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .shop-box .shop-item .right .line2 {
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .shop-box .shop-item .right .line3 {
  font-size: 23rpx;
  font-weight: 400;
  color: #999999;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .goods-box {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  background-color: #f5f5f5;
  padding-bottom: 20rpx;
}
.page .goods-box .goods-item {
  margin-top: 15rpx;
  background-color: #ffffff;
  width: 369rpx;
}
.page .goods-box .goods-item .goods-image {
  width: 369rpx;
  height: 369rpx;
}
.page .goods-box .goods-item .title {
  box-sizing: border-box;
  padding: 0 10rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .goods-box .goods-item .bottom {
  padding: 0 10rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-weight: 400;
  color: #ff2020;
}
.page .goods-box .goods-item .bottom image {
  width: 34rpx;
  height: 34rpx;
}
