.page {
  height: 100%;
  width: 100%;
  position: absolute;
  background-color: #f7f7f7;
}
.page .nothing {
  width: 100%;
  height: 80%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.page .nothing image {
  width: 141rpx;
  height: 141rpx;
}
.page .nothing .nothing-tip {
  font-size: 30rpx;
  font-weight: 400;
  color: #666666;
  margin: 30rpx 0;
}
.page .nothing .nothing-button {
  font-size: 23rpx;
  font-weight: 400;
  color: #ffffff;
  width: 182rpx;
  height: 53rpx;
  background: #f26500;
  border-radius: 7rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page .list-box {
  width: 100%;
  display: flex;
  flex-direction: column;
  padding-bottom: 150rpx;
  background-color: #f7f7f7;
}
.page .list-box .shop-list {
  margin-top: 10rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .list-box .shop-list .shop-title {
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
}
.page .list-box .shop-list .shop-title .left {
  display: flex;
}
.page .list-box .shop-list .shop-title .left image {
  width: 26rpx;
  height: 23rpx;
  margin-right: 10rpx;
}
.page .list-box .shop-list .shop-title .left text {
  font-size: 28rpx;
  font-weight: normal;
  color: #666666;
}
.page .list-box .shop-list .shop-title .right {
  font-size: 26rpx;
  font-weight: normal;
  color: #333333;
}
.page .list-box .shop-list .goods-list {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .list-box .shop-list .goods-list .goods-item {
  width: 100%;
  background-color: #f8f8f8;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}
.page .list-box .shop-list .goods-list .goods-item .check {
  width: 30rpx;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info {
  width: calc(100% - 30rpx);
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-pic {
  width: 162rpx;
  height: 162rpx;
  background: #ffffff;
  border: 1px solid #e5e5e5;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail {
  width: calc(100% - 162rpx);
  display: flex;
  height: 162rpx;
  flex-direction: column;
  justify-content: space-around;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line1 {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line1 .goods-title {
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  opacity: 0.8;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line1 .goods-del {
  position: relative;
  left: 20rpx;
  top: -10rpx;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line1 .goods-del image {
  width: 23rpx;
  height: 29rpx;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line2 {
  display: flex;
  justify-content: space-between;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line2 .line2-left {
  font-size: 21rpx;
  font-weight: 400;
  color: #666666;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line2 .line2-right {
  display: flex;
  background-color: #ffffff;
  width: 141rpx;
  height: 40rpx;
  justify-content: space-between;
  font-size: 23rpx;
  font-weight: 400;
  color: #333333;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line2 .line2-right .number {
  width: calc(100% - 80rpx);
  border-top: 1px solid #fff;
  border-bottom: 1px solid #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line2 .line2-right .num-minus,
.page .list-box .shop-list .goods-list .goods-item .goods-info .goods-detail .line2 .line2-right .num-add {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #d7d7d7;
  border-radius: 5rpx;
}
.page .list-box .shop-list .goods-list .count {
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  width: 100%;
  background-color: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
  font-size: 26rpx;
  font-weight: normal;
  color: #666666;
}
.page .list-box .shop-list .goods-list .count .color {
  color: #F26500;
}
.page .bottom-count {
  width: 100%;
  display: flex;
  justify-content: flex-end;
  position: fixed;
  bottom: 0;
}
.page .bottom-count .count {
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  width: 100%;
  height: 98rpx;
  padding-right: 20rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 26rpx;
  font-weight: normal;
  color: #666666;
}
.page .bottom-count .count .color {
  color: #F26500;
}
.page .bottom-count .count-button {
  width: 223rpx;
  height: 98rpx;
  background: #f26500;
  font-size: 37rpx;
  font-weight: 400;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
}
