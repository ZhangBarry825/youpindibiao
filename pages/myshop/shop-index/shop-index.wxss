.page {
  height: 100%;
  width: 100%;
  position: absolute;
  background-color: #f7f7f7;
  z-index: -2;
}
.page .shop-show {
  height: 100%;
  width: 100%;
}
.page .shop-show .mine-back {
  width: 100%;
  height: 315rpx;
  position: relative;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.page .shop-show .mine-back .left {
  width: 133rpx;
  height: 133rpx;
  border-radius: 50%;
  overflow: hidden;
}
.page .shop-show .mine-back .left image {
  width: 100%;
  height: 100%;
}
.page .shop-show .mine-back .right {
  width: calc(100% - 133rpx);
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
}
.page .shop-show .mine-back .right .line1 {
  font-size: 35rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .shop-show .mine-back .right .line2 {
  font-size: 28rpx;
  font-weight: 400;
  color: #ffffff;
  margin: 10rpx 0;
}
.page .shop-show .mine-back .right .line3 {
  font-size: 23rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .shop-show .mine-back .back-img {
  z-index: -1;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
}
.page .shop-show .statistics {
  width: 100%;
  background-color: #ffffff;
  margin: 20rpx 0;
  display: flex;
  flex-direction: column;
}
.page .shop-show .statistics .line1 {
  border-bottom: 1px solid rgba(0, 0, 0, 0.05);
}
.page .shop-show .statistics .line1,
.page .shop-show .statistics .line2 {
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.page .shop-show .statistics .line1 .item,
.page .shop-show .statistics .line2 .item {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .shop-show .statistics .line1 .item .title,
.page .shop-show .statistics .line2 .item .title {
  font-size: 28rpx;
  font-weight: 400;
  color: #4d4d4d;
}
.page .shop-show .statistics .line1 .item .number,
.page .shop-show .statistics .line2 .item .number {
  font-size: 35rpx;
  font-weight: 400;
  color: #f26500;
  margin: 10rpx 0;
}
.page .shop-show .menu-box image {
  width: 57rpx;
  height: 57rpx;
}
.page .shop-show .menu-box text {
  margin-top: 20rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.page .shop-none {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.page .shop-none .tips {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.page .shop-none .tips image {
  width: 204rpx;
  height: 160rpx;
}
.page .shop-none .tips .line1 {
  margin-top: 50rpx;
  margin-bottom: 15rpx;
  font-size: 36rpx;
  font-weight: 400;
  color: #222222;
}
.page .shop-none .tips .line2 {
  font-size: 30rpx;
  font-weight: 400;
  color: #999999;
}
.page .shop-none .buttons {
  width: 100%;
  padding: 50rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .shop-none .buttons .button {
  margin: 20rpx 0;
  padding: 20rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 600rpx;
  font-size: 24rpx;
  font-weight: 400;
  border-radius: 12rpx;
}
.page .shop-none .buttons .button1 {
  color: #ffffff;
  background: #f26500;
}
.page .shop-none .buttons .button2 {
  background: #ebebeb;
  color: #4d4d4d;
}
