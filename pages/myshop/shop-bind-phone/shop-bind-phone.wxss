.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.page .custom-class {
  padding: 0;
}
.page .button-box {
  width: 100%;
  padding: 50rpx 0;
  display: flex;
  justify-content: center;
}
.page .button-box .button {
  width: 609rpx;
  background: #f26500;
  border-radius: 35rpx;
  padding: 20rpx 0;
  font-size: 28rpx;
  font-weight: 400;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
}
