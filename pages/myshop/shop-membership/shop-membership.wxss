.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
  box-sizing: border-box;
}
.page .tab-class {
  flex-basis: 50% !important;
}
.page .tab-active-class {
  color: #F26500;
}
.page .tip {
  width: 100%;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  display: flex;
}
.page .tip .red {
  color: #FDBF19;
}
.page .items {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .items .item {
  border-bottom: 1px solid rgba(0, 0, 0, 0.05);
  width: 100%;
  background-color: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
}
.page .items .item .left {
  width: 95rpx;
  height: 95rpx;
  margin-right: 20rpx;
}
.page .items .item .left image {
  width: 95rpx;
  height: 95rpx;
  border-radius: 50%;
}
.page .items .item .right {
  width: calc(100% - 95rpx);
  display: flex;
  flex-direction: column;
}
.page .items .item .right .line1 {
  margin-bottom: 20rpx;
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: #1f1f1f;
}
.page .items .item .right .line2,
.page .items .item .right .line3 {
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.page .items .item .right .line4 {
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.page .items .item .right .line4 image {
  width: 150rpx;
  height: 42rpx;
  margin-right: 20rpx;
}
.page .items .item .right .line4 text {
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
}
.page .shop-none {
  width: 100%;
  margin-top: 100rpx;
  height: 60%;
  background-color: #f4f4f4;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.page .shop-none image {
  width: 200rpx;
  height: 200rpx;
}
.page .shop-none .text {
  font-size: 30rpx;
  font-weight: 400;
  color: #030000;
  margin: 50rpx 0;
}
.page .shop-none .none-button {
  width: 212rpx;
  padding: 15rpx 0;
  border: 1px solid #f26500;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  font-weight: 400;
  color: #f26500;
}
