.page {
  height: 100%;
  width: 100%;
  position: absolute;
  background-color: #f7f7f7;
  z-index: -2;
}
.page .mine-back {
  width: 100%;
  height: 315rpx;
  position: relative;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.page .mine-back .searchInput {
  margin: 0;
  padding: 0 20rpx;
  box-sizing: border-box;
  position: absolute;
  top: 10rpx;
  width: 100%;
  z-index: 1;
}
.page .mine-back .searchInput .van-search__content {
  background-color: rgba(255, 255, 255, 0.7);
}
.page .mine-back .info {
  margin-top: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .mine-back .info image {
  width: 94rpx;
  height: 94rpx;
  border-radius: 50%;
}
.page .mine-back .info text {
  margin: 10rpx 0;
  font-size: 28rpx;
  font-weight: 400;
  color: #ffffff;
  padding: 5rpx 20rpx;
  background-color: rgba(56, 56, 56, 0.33);
  border-radius: 20rpx;
}
.page .mine-back .back-img {
  z-index: -1;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
}
.page .menu-box image {
  width: 57rpx;
  height: 57rpx;
}
.page .menu-box text {
  margin-top: 20rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.page .header {
  width: 100%;
  margin: 10rpx 0;
}
.page .header .order-box .color text {
  color: #f26500;
}
.page .header .order-box text {
  font-size: 28rpx;
  font-weight: 400;
  color: #4d4d4d;
}
.page .header .order-box .van-grid-item__content {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.page .header .order-box .van-grid-item__content image {
  margin-left: 10rpx;
  height: 20rpx;
  width: 10rpx;
}
.page .items {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .items .item {
  margin-bottom: 10rpx;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  background-color: #ffffff;
}
.page .items .item .left image {
  width: 246rpx;
  height: 188rpx;
}
.page .items .item .right {
  padding-left: 20rpx;
  box-sizing: border-box;
  width: calc(100% - 246rpx);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .items .item .right .line1 {
  font-size: 28rpx;
  font-weight: 400;
  color: #4d4d4d;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .items .item .right .line2,
.page .items .item .right .line3 {
  padding-right: 20rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 21rpx;
  font-weight: 400;
  color: #808080;
}
.page .items .item .right .line2 .line-left,
.page .items .item .right .line3 .line-left,
.page .items .item .right .line2 .line-right,
.page .items .item .right .line3 .line-right {
  display: flex;
  align-items: center;
}
.page .items .item .right .line2 .red,
.page .items .item .right .line3 .red {
  color: #f26500;
}
.page .items .item .right .line4 .button {
  width: 129rpx;
  padding: 10rpx 0;
  background: #f26500;
  border-radius: 7rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 21rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .items .item .right .line4 .button image {
  width: 21rpx;
  height: 21rpx;
  margin: 0 10rpx;
}
.page .wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}
.page .block {
  width: 374rpx;
  height: 401rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  box-sizing: border-box;
}
.page .block .line1 {
  width: 100%;
  display: flex;
  justify-content: flex-end;
}
.page .block .line1 image {
  width: 30rpx ;
  height: 30rpx;
}
.page .block .line2,
.page .block .line3,
.page .block .line4 {
  width: 100%;
  display: flex;
  justify-content: center;
}
.page .block .lin2 {
  font-size: 28rpx;
  font-weight: 400;
  color: #4d4d4d;
}
.page .block .line3 image {
  width: 204rpx;
  height: 204rpx;
  margin: 30rpx 0;
}
.page .block .line4 {
  font-size: 21rpx;
  font-weight: 400;
  color: #4d4d4d;
}
