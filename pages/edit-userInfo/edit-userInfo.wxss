.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.page .avatar-custom-class {
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
}
.page .avatar-custom-class view {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.page .avatar-custom-class view .title {
  font-size: 30rpx;
  font-weight: 400;
  color: #494949;
}
.page .avatar-custom-class view .value {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.page .avatar-custom-class view .value image {
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
}
.page .item-custom-class {
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #ffffff;
}
.page .item-custom-class .aaa {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.page .item-custom-class .aaa .title {
  font-size: 30rpx;
  font-weight: 400;
  color: #494949;
}
.page .item-custom-class .aaa .input-value {
  display: flex;
  align-items: center;
}
.page .submit-button {
  margin-top: 40rpx;
  width: 100%;
  padding: 30rpx 0;
  background: #f26500;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 35rpx;
  font-weight: 400;
  color: #ffffff;
}
