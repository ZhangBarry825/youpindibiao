.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
}
.page .swiper-box {
  width: 100%;
  height: 435rpx;
  position: relative;
}
.page .swiper-box .slide-image {
  width: 100%;
  height: 100%;
}
.page .goods-info {
  background-color: #ffffff;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
}
.page .goods-info .goods-title {
  width: 100%;
  font-size: 35rpx;
  font-weight: 400;
  color: #333333;
  padding-bottom: 20rpx;
}
.page .goods-info .goods-price {
  display: flex;
  width: 100%;
  flex-direction: row;
}
.page .goods-info .goods-price .left {
  width: calc(100% - 120rpx);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .goods-info .goods-price .left .line1 {
  width: 100%;
  display: flex;
}
.page .goods-info .goods-price .left .line1 .text1 {
  display: flex;
  color: #F26500;
  font-size: 28rpx;
  margin-right: 30rpx;
}
.page .goods-info .goods-price .left .line1 .text2 {
  display: flex;
  color: #999999;
  font-size: 28rpx;
}
.page .goods-info .goods-price .left .line1 .text2 .num {
  text-decoration: line-through;
}
.page .goods-info .goods-price .left .line2 {
  width: 100%;
  display: flex;
  font-size: 28rpx;
  font-weight: 400;
  color: #4d4d4d;
}
.page .goods-info .goods-price .left .line2 .text1 {
  margin-right: 30rpx;
}
.page .goods-info .goods-price .right {
  width: 120rpx;
  height: 88rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #4d4d4d;
  background-color: #ffffff;
  border: none;
  border-left: 1px solid rgba(0, 0, 0, 0.1);
  border-radius: unset;
  line-height: unset;
}
.page .goods-info .goods-price .right image {
  width: 32rpx;
  height: 39rpx;
}
.page .goods-info .goods-price .right::after {
  border: none;
}
.page .specification {
  width: 100%;
  padding: 30rpx 20rpx;
  background-color: #ffffff;
  margin-top: 10rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.page .specification image {
  width: 22rpx;
  height: 35rpx;
}
.page .comment {
  margin-top: 10rpx;
  background-color: #fff;
  padding-bottom: 10rpx;
}
.page .comment .title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.page .comment .title .border {
  margin-right: 20rpx;
  width: 9rpx;
  height: 40rpx;
  background: #e3353f;
}
.page .comment .item {
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  align-items: center;
}
.page .comment .item .line1 {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.page .comment .item .line1 .name {
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.page .comment .item .line1 .stars {
  margin-top: 3rpx;
  margin-left: 10rpx;
  display: flex;
}
.page .comment .item .line2 {
  padding: 10rpx 0;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #666666;
  line-height: 1.5;
}
.page .comment .item .line3 {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.page .comment .item .line3 .image-class {
  margin: 5px 10rpx;
}
.page .comment .item .line4 {
  align-self: flex-start;
  padding: 10rpx 0;
  box-sizing: border-box;
  font-size: 28rpx;
  font-weight: 400;
  color: #9b9b9b;
}
.page .comment .button {
  margin: 20rpx auto;
  width: 309rpx;
  height: 60rpx;
  background: #ffffff;
  border: 1px solid #e41839;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #e3353f;
}
.page .comment .no-comment {
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #666666;
}
.page .goods-detail {
  padding-bottom: 97rpx;
  margin-top: 10rpx;
  background-color: #fff;
}
.page .goods-detail .title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.page .goods-detail .title .border {
  margin-right: 20rpx;
  width: 9rpx;
  height: 40rpx;
  background: #e3353f;
}
.page .goods-detail .content {
  width: 100%;
}
.page .goods-detail .content image {
  width: 100% !important;
}
.page .bottom-box {
  width: 100%;
  height: 97rpx;
  background-color: #ffffff;
  position: fixed;
  bottom: 0;
  display: flex;
}
.page .bottom-box .like {
  width: calc(100% - 572rpx);
  height: 97rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #4d4d4d;
}
.page .bottom-box .like image {
  margin-right: 10rpx;
  width: 35rpx;
  height: 33rpx;
}
.page .bottom-box .add {
  width: 286rpx;
  height: 97rpx;
  background: #f26500;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 33rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .bottom-box .buy {
  width: 286rpx;
  height: 97rpx;
  background: #E41839;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 33rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .icons {
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.page .icons .icon {
  width: 150rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #666666;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .icons .icon image {
  width: 94rpx;
  height: 94rpx;
  margin-bottom: 15rpx;
}
.page .shareCancel {
  width: 100%;
}
.page .Spe-custom-class .close-spe-box {
  position: relative;
}
.page .Spe-custom-class .close-spe-box .image-box {
  z-index: 99;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  width: 50rpx;
  height: 50rpx;
}
.page .Spe-custom-class .close-spe-box .image-box image {
  width: 100%;
  height: 100%;
}
.page .Spe-custom-class .spe-goods-info {
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  box-sizing: border-box;
  position: relative;
}
.page .Spe-custom-class .spe-goods-info .left {
  border: 1px solid #dcdcdc;
  border-radius: 12rpx;
  width: 213rpx;
  height: 224rpx;
  position: relative;
  overflow: hidden;
}
.page .Spe-custom-class .spe-goods-info .left image {
  width: 100%;
  height: 100%;
}
.page .Spe-custom-class .spe-goods-info .right {
  width: calc(100% - 223rpx);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.page .Spe-custom-class .spe-goods-info .right .title {
  width: 100%;
  font-size: 29rpx;
  line-height: 1.5;
  font-weight: 400;
  color: #333333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .Spe-custom-class .spe-goods-info .right .price {
  font-size: 33rpx;
  font-weight: 400;
  color: #ff0404;
}
.page .Spe-custom-class .spe-goods-item {
  width: 100%;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.page .Spe-custom-class .spe-goods-item .title {
  font-size: 28rpx;
  font-weight: 600;
  color: #333333;
  margin-right: 10rpx;
}
.page .Spe-custom-class .spe-goods-item .item {
  margin: 10rpx;
  padding: 10rpx 25rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  font-weight: 400;
  color: #333333;
  border: 1px solid #d2d2d2;
  position: relative;
}
.page .Spe-custom-class .spe-goods-item .item image {
  display: none;
  position: absolute;
  bottom: 0;
  right: 0;
  width: 13rpx;
  height: 13rpx;
}
.page .Spe-custom-class .spe-goods-item .selected {
  border: 1px solid #D0181E;
}
.page .Spe-custom-class .spe-goods-item .selected image {
  display: block;
}
.page .Spe-custom-class .spe-goods-item .number {
  font-size: 22rpx;
  font-weight: 400;
  color: #000000;
  margin: 0 20rpx;
}
.page .Spe-custom-class .spe-goods-item .minus,
.page .Spe-custom-class .spe-goods-item .add {
  margin: 0 10rpx;
  width: 33rpx;
  height: 33rpx;
  background-color: #e9e9e9;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #888888;
}
.page .Spe-custom-class .spe-buttons {
  width: 100%;
  display: flex;
}
.page .Spe-custom-class .spe-buttons .spe-button {
  width: 50%;
  height: 97rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 33rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .Spe-custom-class .spe-buttons .trolley {
  background: #f26500;
}
.page .Spe-custom-class .spe-buttons .buy {
  background: #E41839;
}
.page .Spe-custom-class .spe-goods-num {
  margin-bottom: 30rpx;
}
