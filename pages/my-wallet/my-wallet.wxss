.page {
  height: 100%;
  width: 100%;
  position: absolute;
  background-color: #f7f7f7;
  z-index: -2;
}
.page .mine-back {
  width: 100%;
  height: 315rpx;
  position: relative;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .mine-back .top {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.page .mine-back .top text {
  margin: 20rpx 0;
  font-size: 24rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .mine-back .bottom {
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.page .mine-back .bottom .line1 {
  width: 100%;
  text-align: center;
  margin: 30rpx 0;
  font-size: 30rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .mine-back .bottom .line2 {
  width: 100%;
  text-align: center;
  font-size: 54rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .mine-back .back-img {
  z-index: -1;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
}
.page .buttons {
  width: 100%;
  padding: 50rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page .buttons .button {
  margin: 20rpx 0;
  padding: 20rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 600rpx;
  font-size: 24rpx;
  font-weight: 400;
  border-radius: 12rpx;
}
.page .buttons .button1 {
  color: #ffffff;
  background: #f26500;
}
.page .buttons .button2 {
  background: #ebebeb;
  color: #4d4d4d;
}
