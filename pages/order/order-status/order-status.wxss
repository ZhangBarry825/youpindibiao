.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f4f4f4;
}
.page .top-status {
  width: 750rpx;
  height: 141rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f26500;
  font-size: 30rpx;
  font-weight: 400;
  color: #ffffff;
  opacity: 0.9;
}
.page .top-status image {
  margin: 0 20rpx;
  width: 59rpx;
  height: 54rpx;
}
.page .receiver {
  width: 100%;
  display: flex;
  align-items: center;
  margin: 10rpx 0;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #ffffff;
}
.page .receiver image {
  width: 26rpx;
  height: 40rpx;
  padding: 0 20rpx;
}
.page .receiver .right {
  width: calc(100% - 70rpx);
  padding: 0 15rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.page .receiver .right .line1 {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 33rpx;
  font-weight: 400;
  color: #333333;
}
.page .receiver .right .line2 {
  margin: 15rpx 0;
  width: 100%;
  font-size: 26rpx;
  font-weight: 400;
  color: #666666;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .goods-list {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .goods-list .goods-info {
  width: 100%;
  display: flex;
  padding: 20rpx;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  background-color: #ffffff;
}
.page .goods-list .goods-info .left-info {
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.page .goods-list .goods-info .right-info {
  font-size: 26rpx;
  font-weight: 400;
  color: #1d93d0;
}
.page .goods-list .goods-item {
  width: 100%;
  background-color: #f4f4f4 !important;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
}
.page .goods-list .goods-item .goods-info {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  background-color: #f4f4f4 !important;
}
.page .goods-list .goods-item .goods-info .goods-pic {
  width: 159rpx;
  height: 159rpx;
  background: #ffffff;
  border: 1px solid #e5e5e5;
  border-radius: 7rpx;
}
.page .goods-list .goods-item .goods-info .goods-detail {
  width: calc(100% - 162rpx);
  display: flex;
  height: 162rpx;
  flex-direction: row;
  justify-content: space-between;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-left {
  width: calc(100% - 130rpx);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-left .line1 {
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-left .line2 {
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-right {
  width: 130rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: space-around;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-right .line1 {
  font-size: 26rpx;
  font-weight: 400;
  color: #f26500;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-right .line2 {
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
}
.page .goods-list .goods-item .goods-info .goods-detail .d-right .line3 .seven {
  width: 118rpx;
  height: 47rpx;
  background: #fcc815;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #ffffff;
}
.page .goods-list .count {
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  width: 100%;
  background-color: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
  font-size: 28rpx;
  font-weight: 400;
  color: #131313;
}
.page .goods-list .count .color {
  color: #F26500;
}
.page .custom-class {
  background-color: #f4f4f4;
}
.page .custom-class .title-class {
  font-size: 35rpx;
  font-weight: 400;
  color: #aeaeae;
}
.page .cell-item-custom .cell-item-value {
  font-size: 30rpx;
  font-weight: 400;
  color: #b0b0b0;
}
.page .cell-item-custom .cell-item-title {
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.page .cell-item-custom .red-value {
  color: #F26500;
}
.page .buttons {
  border-top: 10px solid #f4f4f4;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  width: 100%;
  background-color: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
  flex-wrap: wrap;
}
.page .buttons .order-button {
  padding: 10rpx 30rpx;
  border-radius: 6rpx;
  margin: 10rpx;
  font-size: 28rpx;
  font-weight: 400;
}
.page .buttons .order-button1 {
  background: #e8e8e8;
  color: #333333;
}
.page .buttons .order-button2 {
  background: #ffffff;
  border: 1px solid rgba(191, 191, 191, 0.5);
  color: #333333;
}
.page .buttons .order-button3 {
  background: #F26500;
  color: #ffffff;
}
.page .buttons .order-button4 {
  color: #f26500;
}
