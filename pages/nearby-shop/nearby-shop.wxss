.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
}
.page .swiper-box {
  width: 100%;
  height: 435rpx;
  position: relative;
}
.page .swiper-box .slide-image {
  width: 100%;
  height: 100%;
}
.page .shop-info {
  width: 100%;
  background-color: #ffffff;
  margin: 10rpx 0;
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  box-sizing: border-box;
}
.page .shop-info .left {
  width: 146rpx;
  height: 146rpx;
  border-radius: 6rpx;
  overflow: hidden;
}
.page .shop-info .left image {
  width: 100%;
  height: 100%;
}
.page .shop-info .middle {
  width: calc(100% - 276rpx);
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .shop-info .middle .line1 {
  font-size: 33rpx;
  font-weight: 400;
  color: #000000;
  display: flex;
  flex-direction: row;
}
.page .shop-info .middle .line1 .score {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 25rpx;
  font-weight: 400;
  color: #ffa92d;
}
.page .shop-info .middle .line1 .score image {
  margin: 0 2rpx;
  width: 27rpx;
  height: 27rpx;
}
.page .shop-info .middle .line2 {
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.page .shop-info .middle .line3 {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.page .shop-info .middle .line3 .count {
  font-size: 23rpx;
  font-weight: 400;
  color: #999999;
}
.page .shop-info .right {
  border-left: 1px solid rgba(0, 0, 0, 0.1);
  width: 130rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .shop-info .right .line1 {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.page .shop-info .right .line1 image {
  margin: 0 20rpx;
  width: 29rpx;
  height: 27rpx;
}
.page .shop-detail {
  margin-top: 10rpx;
  background-color: #fff;
}
.page .shop-detail .title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  margin-bottom: 10rpx;
}
.page .shop-detail .title .border {
  margin-right: 20rpx;
  width: 9rpx;
  height: 40rpx;
  background: #e3353f;
}
.page .shop-detail .item {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.5);
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.page .shop-detail .item .line {
  width: 100%;
  display: flex;
  flex-direction: row;
}
.page .shop-detail .item .line .name {
  flex-shrink: 0;
}
.page .comment {
  margin-top: 10rpx;
  background-color: #fff;
}
.page .comment .title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.page .comment .title .border {
  margin-right: 20rpx;
  width: 9rpx;
  height: 40rpx;
  background: #e3353f;
}
.page .comment .item {
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  align-items: center;
}
.page .comment .item .line1 {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.page .comment .item .line1 .name {
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.page .comment .item .line1 .stars {
  margin-left: 30rpx;
  display: flex;
}
.page .comment .item .line1 .stars image {
  width: 27rpx;
  height: 25rpx;
  margin: 0 3rpx;
}
.page .comment .item .line2 {
  padding: 10rpx 0;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #666666;
  line-height: 1.5;
}
.page .comment .item .line3 {
  align-self: flex-start;
  padding: 10rpx 0;
  box-sizing: border-box;
  font-size: 28rpx;
  font-weight: 400;
  color: #9b9b9b;
}
.page .comment .item .button {
  margin: 20rpx 0;
  width: 309rpx;
  height: 60rpx;
  background: #ffffff;
  border: 1px solid #e41839;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #e3353f;
}
.page .all-goods {
  margin-top: 10rpx;
  background-color: #fff;
}
.page .all-goods .title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.page .all-goods .title .border {
  margin-right: 20rpx;
  width: 9rpx;
  height: 40rpx;
  background: #e3353f;
}
.page .all-goods .good-items {
  background-color: #f2f2f2;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.page .all-goods .good-items .good-item {
  margin-bottom: 20rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  width: 345rpx;
}
.page .all-goods .good-items .good-item .good-pic {
  width: 345rpx;
  height: 330rpx;
}
.page .all-goods .good-items .good-item .good-pic image {
  width: 100%;
  height: 100%;
}
.page .all-goods .good-items .good-item .title1 {
  margin: 10rpx 0;
  width: 100%;
  padding: 0 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  font-weight: normal;
  color: #464646;
}
.page .all-goods .good-items .good-item .detail {
  padding: 10rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.page .all-goods .good-items .good-item .detail .price {
  display: flex;
  align-items: center;
}
.page .all-goods .good-items .good-item .detail .price .text1 {
  font-size: 27rpx;
  color: #E41839;
}
.page .all-goods .good-items .good-item .detail .price .text2 {
  color: #7F7F7F;
  font-size: 22rpx;
  text-decoration: line-through;
  margin-left: 20rpx;
}
.page .all-goods .good-items .good-item .detail .trolley {
  width: 50rpx;
  height: 50rpx;
}
.page .all-goods .good-items .good-item .detail .trolley image {
  width: 100%;
  height: 100%;
}
