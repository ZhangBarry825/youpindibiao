.page {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.page .custom-class {
  padding: 0;
}
.page .button-box {
  width: 100%;
  padding: 50rpx 0;
  display: flex;
  justify-content: center;
}
.page .button-box .button {
  width: 609rpx;
  background: #f26500;
  border-radius: 35rpx;
  padding: 20rpx 0;
  font-size: 28rpx;
  font-weight: 400;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page .problem {
  width: 100%;
  display: flex;
  justify-content: center;
}
.page .problem .center {
  width: 609rpx;
  padding: 1rpx 0;
  display: flex;
  justify-content: space-between;
}
.page .problem .center text {
  font-size: 28rpx;
  font-weight: 400;
  color: #f26500;
}
.page .wechat {
  margin-top: 50rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.page .wechat .top {
  margin: 40rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page .wechat .top .border {
  width: 150rpx;
  height: 1px;
  background: #dcdcdc;
}
.page .wechat .top .text {
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
}
.page .wechat image {
  width: 75rpx;
  height: 75rpx;
}
