.page {
  position: absolute;
  width: 100%;
  min-height: 100%;
  background-color: #f5f5f5;
}
.page .btn-box {
  width: 100%;
  display: flex;
  justify-content: center;
}
.page .btn-box .btn {
  width: 670rpx;
  margin: 50rpx auto;
}
.page .tip {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}
.page .tip .left {
  width: 80rpx;
  font-size: 25rpx;
  color: #333333;
}
.page .tip .right {
  width: calc(100% - 80rpx);
  white-space: pre-line;
  font-size: 25rpx;
  color: #666666;
}
.page .title-box {
  display: flex;
  align-items: center;
}
.page .title-box .up-img {
  width: 200rpx;
  height: 50rpx;
  margin-left: 50rpx;
}
